    <DataSet Name="Abstracts">
      <Query>
        <DataSourceName>P2RMISDS</DataSourceName>
        <CommandText>SELECT     ViewApplication.LogNumber, ViewApplicationText.BodyText, ViewApplication.ApplicationId
FROM         ViewApplication INNER JOIN
                      ViewApplicationText ON ViewApplication.ApplicationId = ViewApplicationText.ApplicationId
WHERE     (ViewApplicationText.ClientApplicationTextTypeId = 19)</CommandText>
      </Query>
      <Fields>
        <Field Name="LogNumber">
          <DataField>LogNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BodyText">
          <DataField>BodyText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ApplicationId">
          <DataField>ApplicationId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
