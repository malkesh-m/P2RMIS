<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>P2RMISDB</TargetDatabaseName>
    <DeployScriptFileName>Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=(local)\sqlexpress;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>False</BlockOnPossibleDataLoss>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <GenerateSmartDefaults>True</GenerateSmartDefaults>
    <ScriptNewConstraintValidation>False</ScriptNewConstraintValidation>
    <DropObjectsNotInSource>True</DropObjectsNotInSource>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="LinkedServerName">
      <Value>loopback</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="P2RMIS">
      <Value>P2RMIS</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>