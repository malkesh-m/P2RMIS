@model Sra.P2rmis.Web.UI.Models.TransferDataViewModel
@using Sra.P2rmis.Web.Controllers
@{
    ViewBag.PageTitle = "Generate Data";
    const string NoResult = Sra.P2rmis.Web.Controllers.SetupBaseController.Constants.NoDeliverableResultsFound;
}
@Html.Partial("_KendoHeader2020")
<p class="formTitle">@ViewBag.PageTitle</p>
<div id="successMessage"></div>
<div id="warningAlert" class="alert-message"></div>
<div class="row-fluid">
    <div class="span12" id="mainContent" style="padding:20px">
        @Html.Partial("_TabMenu", Model.TabModel)
        @Html.Partial("_DataTransferFilterMenu", Model.FilterModel)

        <h4 class="gridTitle" id="gridTitle">Data Deliverables</h4>
        <div class="formNoResults" id="formNoResults">@NoResult</div>
        <div id="loading"></div>
        <div id="grid"></div>
    </div>
</div>
@Html.IncludeVersionedFiles("/Scripts/Custom/Setup/generateData.js")
@Html.IncludeVersionedFiles("/Scripts/Custom/fileDownload.js")